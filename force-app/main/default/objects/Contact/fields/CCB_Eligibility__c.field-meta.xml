<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CCB_Eligibility__c</fullName>
    <description>Determines if the contact is able to submit a CCB claim.</description>
    <inlineHelpText>Contacts with value &quot;Eligible&quot;  or &quot;Not Eligible for current year&quot; will be able to view or submit rebates. Contacts with value &quot;Not Eligible&quot; or blank will not be able to submit or view rebates</inlineHelpText>
    <label>CCB Eligibility</label>
    <required>false</required>
    <trackFeedHistory>false</trackFeedHistory>
    <trackHistory>true</trackHistory>
    <type>Picklist</type>
    <valueSet>
        <restricted>true</restricted>
        <valueSetDefinition>
            <sorted>false</sorted>
            <value>
                <fullName>Eligible</fullName>
                <default>false</default>
                <label>Eligible</label>
            </value>
            <value>
                <fullName>Not Eligible</fullName>
                <default>false</default>
                <label>Not Eligible</label>
            </value>
            <value>
                <fullName>Not Eligible for current year</fullName>
                <default>false</default>
                <isActive>false</isActive>
                <label>Not Eligible for current year</label>
            </value>
        </valueSetDefinition>
    </valueSet>
</CustomField>
